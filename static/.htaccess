Options -Indexes

RewriteEngine On

RedirectMatch 301 ^.*/feed(/(atom|rss2?))?/?$ http://feeds.feedburner.com/not-a-kernel-guy
RedirectMatch 301 ^.*/(atom|rss2?)\.xml$ http://feeds.feedburner.com/not-a-kernel-guy

RedirectMatch 301 ^/tag/(.*)$ https://blog.not-a-kernel-guy.com/tags/$1

RewriteCond %{HTTPS} !on
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}

ErrorDocument 404 /404.html

<ifModule mod_expires.c>
    ExpiresActive On
    ExpiresDefault "access plus 5 seconds"
    ExpiresByType image/x-icon "access plus 1 month"
    ExpiresByType image/jpeg "access plus 1 month"
    ExpiresByType image/png "access plus 1 month"
    ExpiresByType image/gif "access plus 1 month"
    ExpiresByType application/x-shockwave-flash "access plus 1 month"
    ExpiresByType text/css "access plus 10 minutes"
    ExpiresByType text/javascript "access plus 10 minutes"
    ExpiresByType application/javascript "access plus 10 minutes"
    ExpiresByType application/x-javascript "access plus 10 minutes"
    ExpiresByType text/html "access plus 10 minutes"
    ExpiresByType application/xhtml+xml "access plus 10 minutes"
</ifModule>
