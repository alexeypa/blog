Options -Indexes

RewriteEngine On

RedirectMatch 301 ^.*/feed(/(atom|rss2?))?/?$ http://feeds.feedburner.com/not-a-kernel-guy
RedirectMatch 301 ^.*/(atom|rss2?)\.xml$ http://feeds.feedburner.com/not-a-kernel-guy

RedirectMatch 301 ^/tag/(.*)$ https://blog.not-a-kernel-guy.com/tags/$1

RewriteCond %{HTTPS} !on
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}

ErrorDocument 404 /404.html
